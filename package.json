    "morgan": "^1.9.1",
    "nest-winston": "^1.0.4",
    "nestjs-exceptions": "^1.0.3",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
