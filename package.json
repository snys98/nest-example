    "@schematics/angular": "^7.3.9",
    "@types/express-winston": "^3.0.2",
    "@types/graphql": "^14.2.0",
    "@types/morgan": "^1.7.35",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/passport-local": "^1.0.33",
