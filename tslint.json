{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "no-unused-expression": true
    },
    "rules": {
        "quotemark": false,
        "member-access": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "max-line-length": [
            true,
            240
        ],
        "member-ordering": [
            false
        ],
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "prefer-const": false,
        "align": false,
        "no-consecutive-blank-lines":false
    },
    "rulesDirectory": []
}